<h1>HTML Questions </h1>
<%= render "form" %>
<table>
  <thead>
    <tr>
      <th>Questions:  </th>
      <th> </th>
      <th>Answers:  </th>
    </tr>
  </thead>

  <tbody>
    <% @skeletons.each do |skeleton| %>
    <tr>
      <td><%= skeleton.question %></td>
      <td><%= link_to "Delete", skeleton, method: :delete,
                    confirm: "Are you sure you wish to delete this question?" %></td>
      <td><%= skeleton.answer %></td>
      <td><%= link_to "Delete", skeleton, method: :delete,
                    confirm: "Are you sure you wish to delete this answer?" %></td>
      <td><%= link_to "Show", skeleton_path(skeleton) %></td>

    </tr>
    <% end %>
  </tbody>
</table>

      <%= link_to "HTML Home", new_skeleton_path %>
