<h1>HTML Questions </h1>
<table>
  <thead>
    <tr>
      <th>  Questions:  </th>
      <th>  Answers:  </th>
    </tr>
  </thead>
  <tbody>
    <%@skeletons.each do |skeleton| %>
    <tr>
      <td><%= skeleton.question %></td>
      <td><%= skeleton.answer %></td>
      <%= link_to "Show", skeleton_path(skeleton) %> |
      <%= link_to "Update", edit_skeleton_path(person) %> |
      <%= link_to "Delete", skeleton, method: :delete,
                    confirm: "Are you sure you wish to delete this?" %>
  </tbody>
</table>
<% end %>
<%= link_to "HTML Home", new_skeleton_path %>
